<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
    <title>@ViewBag.Title – HitechCraft - Multiserver Complex</title>
    <meta name="Description" content="Мультисерверный игровой проект Minecraft. Только у нас сервера различной направленности. Максимум возможностей при минимуме доната.">
    <meta name="Keywords" content="minecraft, майн, hitechcraft, support, worlds, parallel, extentions, minecraft, industrial, экстеншинс, паралельные, миры, дома, постройки, много, лавы, хороший, сервер, большой, онлайн, майник, сеты, магия, magic, индустриал">
    <link rel="icon" type="image/png" href="../../favicon.ico" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    <script src='https://www.google.com/recaptcha/api.js'></script>
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="main">
        @Html.Partial("_NavBar")
        <div class="container">
            @RenderBody()
        </div>
        @Html.Partial("_Footer")
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
